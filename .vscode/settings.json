{
    "todohighlight.keywords": [
        {
            "text": "TODO",
            "color": "#282C34",
            "backgroundColor": "#E5C07B",
        },
        {
            "text": "FIXME",
            "color": "#282C34",
            "backgroundColor": "#EF596F",
        },
        {
            "text": "NOTE",
            "color": "#282C34",
            "backgroundColor": "#61AFEF"
        },
        {
            "text": "TEMP",
            "color": "#282C34",
            "backgroundColor": "#89CA78"
        }
    ],
    "editor.rulers": [
        {
            "column": 100,
            "color": "#343944",
        }
    ],
    "C_Cpp.codeAnalysis.clangTidy.enabled": true,
    "C_Cpp.files.exclude": {
        "**/.idea": true,
        "**/.xcode": true,
        "**/.vscode": true,
        "**/node_modules": true,
        "**/_deps": true,
        "**/build": true,
        "**/.vs": true
    },
    "C_Cpp.codeAnalysis.clangTidy.useBuildPath": true,
    "C_Cpp.codeAnalysis.clangTidy.path": "clang-tidy-mp-20",
}